{
  "expo": {
    "name": "app-menu",
    "slug": "app-menu",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.anonymous.appmenu"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      ["expo-quick-actions", {
        "androidIcons": {
          "help_icon": {
            "foregroundImage": "./assets/images/help.png",
            "backgroundColor": "#FFFFFF"
          }
        }
      }]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
